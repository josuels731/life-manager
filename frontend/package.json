{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --port 3000",
    "create-build": "tsc && vite build",
    "copy-build": "(if not exist ..\\public mkdir ..\\public) && del /s /q /f ..\\public && xcopy /s /y .\\dist\\* ..\\public",
    "build": "npm run create-build && npm run copy-build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}